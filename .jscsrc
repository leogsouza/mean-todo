{
  "fileExtensions": [
    ".js",
    ".jsx"
  ],
  "preset": "airbnb",

  "validateLineBreaks": "CRLF",
  "requireTrailingComma": {
    "ignoreSingleValue": true,
    "ignoreSingleLine": true
  },

  "esprima": "esprima-fb",
  "excludeFiles": [
    "node_modules/**",
    "./example/node_modules/**",
    "./example/public/bundle.js"
  ]
}
